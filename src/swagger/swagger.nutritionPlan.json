{
    "openapi": "3.0.0",
    "info": {
      "title": "AI Recommendation API",
      "description": "API endpoints for AI-generated recommendations",
      "version": "1.0.0"
    },
    "servers": [
      {
        "url": "http://localhost:8080",
        "description": "Development server"
      }
    ],
    "paths": {
      "/ai/{userId}/nutrition-plan": {
        "get": {
          "tags": ["AI Recommendations"],
          "summary": "Get AI-generated nutrition plan",
          "description": "Retrieves a personalized nutrition plan generated by AI for a specific user",
          "parameters": [
            {
              "name": "userId",
              "in": "path",
              "required": true,
              "description": "The unique identifier of the user",
              "schema": {
                "type": "string",
                "example": "67d3968af0eafbf665b44e7d"
              }
            }
          ],
          "security": [
            {
              "bearerAuth": []
            }
          ],
          "responses": {
            "200": {
              "description": "Successful nutrition plan retrieval",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "status": {
                        "type": "string",
                        "example": "success"
                      },
                      "data": {
                        "type": "object",
                        "properties": {
                          "plan": {
                            "type": "object",
                            "properties": {
                              "name": {
                                "type": "string",
                                "example": "Custom Nutrition Plan"
                              },
                              "meals": {
                                "type": "array",
                                "items": {
                                  "type": "object",
                                  "properties": {
                                    "mealType": {
                                      "type": "string",
                                      "example": "Breakfast",
                                      "enum": ["Breakfast", "Lunch", "Dinner", "Snack 1", "Snack 2", "Snack 3"]
                                    },
                                    "recipes": {
                                      "type": "array",
                                      "items": {
                                        "type": "object",
                                        "properties": {
                                          "name": {
                                            "type": "string",
                                            "example": "Oatmeal with Berries"
                                          },
                                          "ingredients": {
                                            "type": "array",
                                            "items": {
                                              "type": "string"
                                            },
                                            "example": ["50g Rolled Oats", "200ml Almond Milk", "50g Mixed Berries", "1 tbsp Chia Seeds"]
                                          },
                                          "portionSize": {
                                            "type": "string",
                                            "example": "300g"
                                          },
                                          "calories": {
                                            "type": "integer",
                                            "example": 350
                                          },
                                          "macros": {
                                            "type": "object",
                                            "properties": {
                                              "protein": {
                                                "type": "string",
                                                "example": "10g"
                                              },
                                              "carbs": {
                                                "type": "string",
                                                "example": "60g"
                                              },
                                              "fats": {
                                                "type": "string",
                                                "example": "10g"
                                              }
                                            }
                                          },
                                          "cookingTips": {
                                            "type": "string",
                                            "example": "Cook oats in almond milk until desired consistency. Top with berries and chia seeds."
                                          }
                                        }
                                      }
                                    }
                                  }
                                }
                              },
                              "dailyTargets": {
                                "type": "object",
                                "properties": {
                                  "calories": {
                                    "type": "integer",
                                    "example": 2000
                                  },
                                  "protein": {
                                    "type": "string",
                                    "example": "80g"
                                  },
                                  "carbs": {
                                    "type": "string",
                                    "example": "250g"
                                  },
                                  "fats": {
                                    "type": "string",
                                    "example": "70g"
                                  }
                                }
                              },
                              "hydration": {
                                "type": "object",
                                "properties": {
                                  "dailyWater": {
                                    "type": "string",
                                    "example": "2 liters"
                                  },
                                  "timingRecommendations": {
                                    "type": "array",
                                    "items": {
                                      "type": "string"
                                    },
                                    "example": ["Throughout the day"]
                                  }
                                }
                              },
                              "supplements": {
                                "type": "array",
                                "items": {
                                  "type": "string"
                                },
                                "example": []
                              },
                              "shoppingList": {
                                "type": "array",
                                "items": {
                                  "type": "string"
                                },
                                "example": ["Rolled Oats", "Almond Milk", "Mixed Berries", "Chia Seeds"]
                              },
                              "progressiveOverload": {
                                "type": "object",
                                "properties": {
                                  "week1": {
                                    "type": "object",
                                    "properties": {
                                      "calories": {
                                        "type": "integer",
                                        "example": 1800
                                      }
                                    }
                                  },
                                  "week2": {
                                    "type": "object",
                                    "properties": {
                                      "calories": {
                                        "type": "integer",
                                        "example": 1900
                                      }
                                    }
                                  },
                                  "week3": {
                                    "type": "object",
                                    "properties": {
                                      "calories": {
                                        "type": "integer",
                                        "example": 2000
                                      }
                                    }
                                  },
                                  "week4": {
                                    "type": "object",
                                    "properties": {
                                      "calories": {
                                        "type": "integer",
                                        "example": 2100
                                      }
                                    }
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            },
            "401": {
              "description": "Unauthorized, invalid or missing token",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "status": {
                        "type": "string",
                        "example": "error"
                      },
                      "message": {
                        "type": "string",
                        "example": "Unauthorized access"
                      }
                    }
                  }
                }
              }
            },
            "404": {
              "description": "User not found",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "status": {
                        "type": "string",
                        "example": "error"
                      },
                      "message": {
                        "type": "string",
                        "example": "User not found"
                      }
                    }
                  }
                }
              }
            },
            "500": {
              "description": "AI service error",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "status": {
                        "type": "string",
                        "example": "error"
                      },
                      "message": {
                        "type": "string",
                        "example": "Error generating nutrition plan"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "components": {
      "securitySchemes": {
        "bearerAuth": {
          "type": "http",
          "scheme": "bearer",
          "bearerFormat": "JWT"
        }
      }
    }
  }